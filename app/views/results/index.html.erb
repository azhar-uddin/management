<h1>Listing Result</h1>
<table>
  <tr>
    <th>Student Id</th>
	<th>Subject</th>
	<th>Marks</th>
  </tr>
 
  <% @result.each do |result| %>
    <tr>
      <td><%= result.user_id %></td>
	  <td><%= result.sname %></td>
	  <td><%= result.marks %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'New Result', new_result_path %>